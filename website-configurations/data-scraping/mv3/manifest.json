{
    "name": "Download Script Resources",
    "description": "Downloads webpage rseources.",
    "version": "2.0",
    "manifest_version": 3,
    "devtools_page": "devtools.html",
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["https://extension/*", "https://*/*", "http://*/*", "<all_urls>"] ,
    "permissions": [
      "downloads", 
      "tabs", 
      "scripting", 
      "pageCapture", 
      "activeTab", 
      "contextMenus", 
      "storage"
    ]
  }